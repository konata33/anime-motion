{
  "name": "@animemotion/vue",
  "version": "0.0.1",
  "packageManager": "pnpm@8.13.0",
  "description": "",
  "type": "module",
  "keywords": [
    "vue",
    "motion",
    "motionone"
  ],
  "license": "MIT",
  "homepage": "",
  "author": "Konata33",
  "files": [
    "dist"
  ],
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs"
    },
    "./types": {
      "types": "./dist/types.d.ts"
    }
  },
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "unbuild",
    "clean": "rimraf ./dist && rimraf ./node_modules",
    "dev": "esno watch.ts",
    "dev:stub": "unbuild --stub",
    "playground": "pnpm --filter @animemotion/playground-vue dev",
    "release": "pnpm build && pnpm bumpp --commit='version(vue): release %s' --no-tag && pnpm publish",
    "test": "vitest",
    "test:watch": "vitest --watch"
  },
  "peerDependencies": {
    "vue": ">=3.3.0"
  },
  "dependencies": {
    "@motionone/dom": "^10.18.0",
    "@sxzz/eslint-config": "^3.12.1"
  },
  "devDependencies": {
    "@types/node": "^20.10.6",
    "@vitejs/plugin-vue": "^5.0.2",
    "@vitest/coverage-v8": "^1.1.1",
    "@vue-macros/volar": "^0.18.8",
    "@vue/test-utils": "^2.4.3",
    "bumpp": "^9.2.1",
    "eslint": "^8.56.0",
    "jsdom": "^23.0.1",
    "lint-staged": "^15.2.0",
    "simple-git-hooks": "^2.9.0",
    "typescript": "^5.3.3",
    "unplugin-vue-macros": "^2.7.7",
    "vite": "^5.0.10",
    "vite-plugin-dts": "^3.7.0",
    "vitest": "^1.1.1",
    "vue": "^3.4.3"
  },
  "engines": {
    "node": ">=18"
  },
  "lint-staged": {
    "*": "pnpm eslint . --fix"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  }
}
